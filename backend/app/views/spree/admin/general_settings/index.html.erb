<!-- 						<tr>
 							<td><%=excel.part_num%></td>
							<td><%=excel.part_num%></td>
							<td><%=excel.description%></td>
							<td><%=excel.price%></td>
							<td><%=excel.ForSale%></td>
							<td></td> -->
							<!--%= button Spree.t('actions.create'), 'ok', 'submit', {class: 'btn-success'} %-->
						<!-- </tr> -->
					</fieldset>
						<% end %>
				</tbody>
			</table>	


		</div>
	</div>
	
</div>