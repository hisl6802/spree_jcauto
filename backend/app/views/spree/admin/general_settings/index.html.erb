<!-- Lists all of the parts which can be created. -->
<div class="container">
	<div class="row">
		<div class="col-md-12 text-center">
			<h2>Products Ready to be created</h2>

			<table class="table sortable">
				<thead>
					<tr>
					<th>Part #</th>
					<th>sku</th>
					<th>Description</th>
					<th>Price</th>
					<th>Available on</th>
					<th>Shipping category</th>
					<th colspan="2">Make a Product</th>
					</tr>
				</thead>
				<tbody>
					<%= form_for [:admin, @excel], :html => { :multipart => true } do |f| %>
  					<fieldset data-hook="new_product">
    					<%= f.field_container :name, :class => ['form-group'] do %>
      					<%= f.label :name, Spree.t(:name) %> <span class="required">*</span>
      					<%= f.text_field :name, :class => 'form-control title' %>
      					<%= f.error_message_on :name %>
    					<% end %>
