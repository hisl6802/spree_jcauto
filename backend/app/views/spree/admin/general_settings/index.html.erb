<!-- Lists all of the parts which can be created. -->
<div class="container">
	<div class="row">
		<div class="col-md-12 text-center">
			<h2>Products Ready to be created</h2>

			<table class="table sortable">
				<thead>
					<tr>
					<th>Part #</th>
					<th>sku</th>
					<th>Description</th>
					<th>Price</th>
					<th>Available on</th>
					<th>Shipping category</th>
					<th colspan="2">Make a Product</th>
					</tr>
				</thead>
				<tbody>
						<% @excels.each do |excel| %>
						<tr>
 							<td><%=excel.part_num%></td>
							<td><%=excel.part_num%></td>
							<td><%=excel.description%></td>
							<td><%=excel.price%></td>
							<td><%=excel.ForSale%></td>
							<td><%=excel.Quantity%></td>
							<td><%= link_to 'Create', new_admin_product_url(excel) %></td>
 							<td><%=link_to 'Remove', edit_admin_general_settings(excel), method: :delete, data: { confirm: 'Are you sure?' } %></td>						
 						</tr>
					<% end %>
				</tbody>
			</table>	
		</div>
	</div>
	
</div>